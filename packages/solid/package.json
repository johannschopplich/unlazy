{
  "name": "@unlazy/solid",
  "version": "0.8.9",
  "packageManager": "pnpm@8.3.1",
  "description": "Solid lazy loading library for placeholder images",
  "author": "Johann Schopplich <pkg@johannschopplich.com>",
  "license": "MIT",
  "homepage": "https://unlazy.byjohann.dev",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/johannschopplich/unlazy.git",
    "directory": "packages/solid"
  },
  "bugs": {
    "url": "https://github.com/johannschopplich/unlazy/issues"
  },
  "keywords": [
    "blurhash",
    "lazy",
    "loading",
    "lozad",
    "placeholder",
    "responsive",
    "thumbhash",
    "solid"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    },
    "./solid": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.jsx"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "pnpm -C playground run dev",
    "dev:build": "pnpm -C playground run build",
    "test:types": "tsc --noEmit"
  },
  "peerDependencies": {
    "solid-js": "^1.7.3"
  },
  "dependencies": {
    "unlazy": "workspace:*"
  },
  "devDependencies": {
    "@types/node": "^18.16.3",
    "eslint-plugin-solid": "^0.12.1",
    "solid-js": "^1.7.3",
    "tsup": "^6.7.0",
    "tsup-preset-solid": "^0.1.8",
    "vite-plugin-solid": "^2.7.0"
  }
}
