{
  "name": "@unlazy/vue",
  "type": "module",
  "version": "0.8.9",
  "packageManager": "pnpm@8.3.1",
  "description": "Vue lazy loading library for placeholder images",
  "author": "Johann Schopplich <pkg@johannschopplich.com>",
  "license": "MIT",
  "homepage": "https://unlazy.byjohann.dev",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/johannschopplich/unlazy.git",
    "directory": "packages/vue"
  },
  "bugs": {
    "url": "https://github.com/johannschopplich/unlazy/issues"
  },
  "keywords": [
    "blurhash",
    "lazy",
    "loading",
    "lozad",
    "placeholder",
    "responsive",
    "thumbhash",
    "vue"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs"
    },
    "./components": {
      "types": "./dist/components/index.d.ts",
      "import": "./dist/components/index.mjs"
    }
  },
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "*",
        "dist/*",
        "dist/*.d.ts",
        "dist/*/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "pnpm -C playground run dev",
    "dev:build": "pnpm -C playground run build",
    "test:types": "vue-tsc --noEmit"
  },
  "dependencies": {
    "unlazy": "workspace:*"
  },
  "devDependencies": {
    "@types/node": "^18.16.5",
    "@vitejs/plugin-vue": "^4.2.1",
    "vue": "^3.2.47"
  }
}
